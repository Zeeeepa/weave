          "title": "Name"
        },
        "description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Description"
        },
        "table": {
          "title": "Table",
          "type": "string"
        },
        "label": {
          "title": "Label",
          "type": "string"
        },
        "definition": {
          "$ref": "#/$defs/SavedViewDefinition"
        }
      },
      "required": [
        "table",
        "label",
        "definition"
      ],
      "title": "SavedView",
      "type": "object"
    },
    "SavedViewDefinition": {
      "properties": {
        "filter": {
